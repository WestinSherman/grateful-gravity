---
import BaseLayout from "../layouts/BaseLayout.astro";

interface Session {
    url: string;
    frontmatter: {
        title: string;
    };
}

const allseshs: Session[] = Object.values(
    import.meta.glob<{ url: string; frontmatter: { title: string } }>(
        "./seshs/*.mdx",
        { eager: true },
    ),
);
const pageTitle = "StarMetal Sessions";
---

<nav class="navbar">
    <ul>
        <div class="home"><li><a href="/">Home</a></li></div>
        <div class="seshs"><li><a href="/seshs/">Seshs</a></li></div>
        <div class="characters">
            <li><a href="/characters/">Characters</a></li>
        </div>
        <div class="links">
            <li><a href="/usefulLinks/">Useful Links</a></li>
        </div>
    </ul>
</nav>
